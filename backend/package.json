{
  "name": "socialflow-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "build": "npm install && npx prisma generate",
    "render:start": "npx prisma migrate deploy && npm start",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node prisma/seed.js",
    "docker:build": "docker build -t socialflow-backend .",
    "docker:run": "docker run -p 5000:5000 socialflow-backend",
    "production": "NODE_ENV=production node src/index.js"
  },
  "dependencies": {
    "@huggingface/inference": "^4.8.0",
    "@prisma/client": "^5.7.0",
    "analytics-node": "^6.2.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "openai": "^5.23.0",
    "stripe": "^18.5.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0"
  }
}
